<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorStackView">
        <attr name="originColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="zjjText">
        <attr name="zjjText" format="string"/>
        <attr name="zjjTextColor" format="color"/>
        <attr name="zjjTextSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="QQStepView">
        <attr name="text" format="string"/>
        <attr name="outlineColor" format="color"/>
        <attr name="inlineColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="maxStep" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CircleProgressViewWithText">
        <attr name="CircleProgressViewText" format="string"/>
        <attr name="CircleProgressViewOutlineColor" format="color"/>
        <attr name="CircleProgressViewInlineColor" format="color"/>
        <attr name="CircleProgressViewTextSize" format="dimension"/>
        <attr name="CircleProgressViewTextColor" format="color"/>
        <attr name="CircleProgressViewBorderWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <attr name="normalImage" format="reference"/>
        <attr name="selectedImage" format="reference"/>
        <attr name="starSpace" format="dimension"/>
        <attr name="starNum" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CircleView">
        <attr name="circle_color" format="color"/>
    </declare-styleable>
</resources>